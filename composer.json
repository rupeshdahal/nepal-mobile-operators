{
    "name": "rupeshdahal/nepal-mobile-operators",
    "description": "A Laravel package to identify mobile operators in Nepal based on number prefixes",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Rupesh Dahal",
            "email": "rupeshdahal15@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^7.4|^8.0",
        "illuminate/support": "^8.0|^9.0|^10.0|^11.0"
    },
    "require-dev": {
       "phpunit/phpunit": "^9.0|^10.0|^11.0",
        "orchestra/testbench": "^6.0|^7.0|^8.0|^9.0"
    },
    "autoload": {
        "psr-4": {
            "RupeshDai\\NepalMobileOperators\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "RupeshDai\\NepalMobileOperators\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "RupeshDai\\NepalMobileOperators\\NepalMobileOperatorsServiceProvider"
            ],
            "aliases": {
                "NepalMobileOperators": "RupeshDai\\NepalMobileOperators\\Facades\\NepalMobileOperators"
            }
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    }
}
